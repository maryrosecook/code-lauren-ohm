CodeLauren {
  Start = Literal | Lambda

  Lambda = "{" Parameters Expressions "}"

  Expressions = listOf<Expression, space>

  Parameters = listOf<Parameter, space>
  Parameter = "?" identifier

  Expression = Literal

  Literal = number | string | identifier

  number = "-"? digit+ ("." digit+)?
  string = "\"" (alnum | "-" | "." | "," | "#" | space)* "\""
  identifier = ~keyword letter+ alnum*

  keyword = "if" | "elseif" | "else" | "forever" | "true" | "false"
}
